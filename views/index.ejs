    }

    .a13 {
      width: 45%;
      padding-left: 40px;
    }

    .a14 {
      font-size: 56px;
      font-weight: 700;
      color: #c8205a;
      margin-bottom: 24px;
      line-height: 1.1;
    }

    .a15 {
      font-size: 20px;
      line-height: 1.4;
      color: #c8205a;
      margin-bottom: 32px;
    }

    .a16 {
