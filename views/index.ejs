    }

    .a18-left-content {
      width: 40%;
      padding-right: 40px;
    }

    .a18-heading {
      font-size: 54px;
      font-weight: 700;
      color: #0e6e73;
      margin-bottom: 20px;
      line-height: 1.1;
    }

    .a18-subheading {
      font-size: 20px;
      line-height: 1.5;
      color: #0e6e73;
      margin-bottom: 30px;
    }

    .a18-button {
      background-color: #e60023;
      color: white;
      border: none;
      border-radius: 24px;
      padding: 12px 24px;
      font-size: 16px;
      font-weight: bold;
      cursor: pointer;
      transition: background-color 0.3s ease;
    }

    .a18-button:hover {
      background-color: #d50021;
    }

    .a18-right-content {
      width: 60%;
      display: grid;
      grid-template-columns: repeat(2, 1fr);
      gap: 20px;
      position: relative;
    }

    .a18-card {
      border-radius: 32px;
      overflow: hidden;
      position: relative;
    }

    .a18-card img {
      width: 100%;
      height: 100%;
      object-fit: cover;
    }

    .a18-card-text {
      position: absolute;
      bottom: 30px;
      left: 20px;
      color: white;
      font-weight: bold;
      font-size: 24px;
      text-shadow: 0 2px 4px rgba(0, 0, 0, 0.5);
      max-width: 80%;
    }

    .a18-card-large {
      grid-column: 1;
      grid-row: 1 / span 3;
      aspect-ratio: 1 / 1.3;
    }

    .a18-card-small {
      aspect-ratio: 1 / 0.8;
    }

    .a18-card-small-1 {
      grid-column: 2;
      grid-row: 1;
    }

    .a18-card-small-2 {
      grid-column: 2;
      grid-row: 2;
    }

    .a18-card-small-3 {
      grid-column: 2;
      grid-row: 3;
    }

    .a18-fern-gallery {
      position: absolute;
      bottom: 100px;
      left: 20px;
      display: flex;
      gap: 8px;
    }

    .a18-mini-image {
      width: 80px;
      height: 80px;
      border-radius: 16px;
      overflow: hidden;
    }

    .a18-mini-image img {
      width: 100%;
      height: 100%;
      object-fit: cover;
    }

    @media screen and (max-width: 1024px) {
      .a18-container {
        flex-direction: column;
        padding: 30px;
      }

      .a18-left-content {
        width: 100%;
        padding-right: 0;
        margin-bottom: 40px;
        text-align: center;
      }

      .a18-right-content {
        width: 100%;
      }

      .a18-heading {
        font-size: 44px;
      }
    }

    @media screen and (max-width: 768px) {
      .a18-right-content {
        display: flex;
        flex-direction: column;
        gap: 16px;
      }

      .a18-card {
        width: 100%;
      }

      .a18-card-large {
        aspect-ratio: 1 / 0.8;
      }

      .a18-fern-gallery {
        position: relative;
        bottom: auto;
        left: auto;
        margin-top: -70px;
        margin-left: 20px;
        z-index: 10;
      }

      .a18-mini-image {
        width: 60px;
        height: 60px;
      }

      .a18-card-text {
        font-size: 20px;
      }
    }

    @media screen and (max-width: 480px) {
      .a18-container {
        padding: 20px;
      }

      .a18-heading {
        font-size: 36px;
      }

      .a18-subheading {
        font-size: 16px;
      }

      .a18-card-text {
        font-size: 18px;
        bottom: 20px;
        left: 15px;
      }

