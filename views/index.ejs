      color: #5a287d;
      font-weight: bold;
    }

    .a7::placeholder {
      color: #5a287d;
      font-weight: bold;
    }

    .a8 {
      width: 260px;
      height: 260px;
      top: 40px;
      left: 0;
      z-index: 1;
    }

    .a9 {
      width: 230px;
      height: 230px;
