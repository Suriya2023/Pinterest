        text-align: center;
      }

      .a14 {
        font-size: 44px;
      }
    }

    @media screen and (max-width: 768px) {
      .a2 {
        height: 400px;
      }

      .a8 {
        width: 200px;
        height: 200px;
      }

      .a9 {
        width: 170px;
        height: 170px;
      }

      .a10 {
        width: 160px;
        height: 220px;
        left: 60px;
      }

      .a11 {
        width: 140px;
        height: 140px;
      }

      .a12 {
        width: 220px;
        height: 220px;
      }

      .a5 {
        width: 250px;
      }
    }

    @media screen and (max-width: 480px) {
      .a1 {
        padding: 10px;
      }

      .a2 {
        height: 350px;
      }

      .a8 {
        width: 160px;
        height: 160px;
      }

      .a9 {
        width: 140px;
        height: 140px;
        right: 30px;
      }

      .a10 {
        width: 120px;
        height: 180px;
        left: 40px;
      }

      .a11 {
        width: 100px;
        height: 100px;
      }

      .a12 {
        width: 180px;
        height: 180px;
        left: 20px;
      }

      .a5 {
        width: 200px;
        padding: 12px 16px;
      }

      .a14 {
        font-size: 36px;
      }

      .a15 {
        font-size: 16px;
      }

      .a16 {
        padding: 12px 24px;
        font-size: 16px;
      }
    }

    .a18body {
      background-color: #ffebf0;
      overflow-x: hidden;
    }

    .a18-container {
      display: flex;
      justify-content: space-between;
      align-items: center;
      padding: 40px;
      min-height: 100vh;
