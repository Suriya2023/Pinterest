    <script>
        //   hodeNotification
        let DomNone = document.getElementById("DomNone")
        let NontficationOn = document.getElementById('NontficationOn')
        let OnopenNotification = document.getElementById('OnopenNotification')
        let OnopenOnhome = document.getElementById('OnopenOnhome')
        OnopenNotification.onclick = () => {
            DomNone.style.display = "none"
            NontficationOn.style.display = "block"
        }
        OnopenOnhome.onclick = () => {
            DomNone.style.display = "block"
            NontficationOn.style.display = "none"

        }


        const profileMenuBtn = document.getElementById('profileMenuBtn');
        const profileDropdown = document.getElementById('profileDropdown');

        profileMenuBtn.addEventListener('click', function () {
            profileDropdown.classList.toggle('active');
        });

        document.addEventListener('click', function (event) {
            if (!profileMenuBtn.contains(event.target) && !profileDropdown.contains(event.target)) {
                profileDropdown.classList.remove('active');
            }
        });

        const sidebarIcons = document.querySelectorAll('.sidebar-icon:not(.logo)');
        const tabItems = document.querySelectorAll('.tab-item');

        sidebarIcons.forEach((icon, index) => {
            icon.addEventListener('click', function () {
                sidebarIcons.forEach(i => i.classList.remove('active'));
                this.classList.add('active');

                if (index < tabItems.length) {
                    tabItems.forEach(t => t.classList.remove('active'));
                    tabItems[index].classList.add('active');
                }
            });
        });

        tabItems.forEach((tab, index) => {
            tab.addEventListener('click', function () {
                tabItems.forEach(t => t.classList.remove('active'));
                this.classList.add('active');

                if (index < sidebarIcons.length) {
                    sidebarIcons.forEach(i => i.classList.remove('active'));
                    sidebarIcons[index].classList.add('active');
                }
            });
        });
    </script>
